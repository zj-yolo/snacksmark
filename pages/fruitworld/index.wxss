.snacksmark-fruitworld-banner {
  height: 340rpx;
}
.snacksmark-fruitworld-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 30rpx 20rpx;
}
.snacksmark-fruitworld-list .snacksmark-fruitworld-list-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 49%;
  height: 400rpx;
  background-color: #f5f6f5;
  border-radius: 20rpx;
  margin-top: 10rpx;
}
.snacksmark-fruitworld-list .snacksmark-fruitworld-list-item navigator {
  height: 200rpx;
  width: 200rpx;
  margin-bottom: 20rpx;
}
.snacksmark-fruitworld-list .snacksmark-fruitworld-list-item view {
  font-size: 30rpx;
}
